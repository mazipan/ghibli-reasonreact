{
  "name": "ghibli-reasonreact",
  "private": true,
  "version": "0.0.0",
  "description": "Ghibli studio's film explorer built with ReasonReact",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=production yarn build",
    "start": "yarn run dev | yarn run bsb-watch",
    "dev": "NODE_ENV=development webpack-dev-server",
    "static": "node scripts/serveStatic.js",
    "bsb-watch": "bsb -make-world -w",
    "build": "yarn clean:dist && bsb -make-world && NODE_ENV=production webpack",
    "bsb-build": "bsb -make-world",
    "upload": "./s3_upload.sh",
    "analyze": "NODE_ENV=analyze yarn build; analyze-bundle -b dist/build/main.js -s dist/build/stats.json",
    "clean": "bsb -clean-world",
    "clean:dist": "rimraf dist"
  },
  "keywords": [],
  "author": "Irfan Maulana <github.com/mazipan>",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "bs-fetch": "^0.3.1",
    "bs-webapi": "^0.14.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "bs-platform": "5.0.3",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electrode-bundle-analyzer": "^1.0.1",
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.3",
    "rollup-loader": "^0.3.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "upath": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "webpack-cli": "3.3.0",
    "webpack-closure-compiler": "^2.1.6",
    "webpack-common-shake": "^2.1.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-shell-plugin": "^0.5.0",
    "webpack-stats-plugin": "^0.2.1"
  }
}
